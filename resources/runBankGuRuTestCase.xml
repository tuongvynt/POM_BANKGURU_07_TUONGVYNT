<?xml version="1.0" encoding="UTF-8"?>
<suite name="Bank GuRu" parallel="false">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="commons.ExtentReportListener" />
	</listeners>

	<test name="Run On Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.bankguru.account.RegisterLogin_Global_Register" />
			<class name="com.bankguru.account.RegisterLogin_Level6_DynamicLocator_RestParameter" />
		</classes>
	</test>

</suite> <!-- Suite -->
